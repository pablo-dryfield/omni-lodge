        }
    } catch (err) {
        console.error("Erreur pendant la vérification :", err);
    }
}


class InscriptionWidget {
            constructor() {
                this.init();
            }

            init() {
                this.bindEvents();
            }

            bindEvents() {
                // Ouvrir le modal
                document.getElementById('inscriptionTrigger').addEventListener('click', () => {
                    this.openModal();
                });

                // Fermer le modal
                document.getElementById('inscriptionClose').addEventListener('click', () => {
                    this.closeModal();
                });

                // Fermer en cliquant sur l'overlay
                document.getElementById('inscriptionOverlay').addEventListener('click', (e) => {
                    if (e.target === e.currentTarget) {
                        this.closeModal();
                    }
                });
/*
                // Soumettre le formulaire
                document.getElementById('submitBtninscription').addEventListener('click', () => {
                    this.submitInscription();
                });
*/
                document.querySelectorAll('[data-action="openConnexion"]').forEach(el => {
                        el.addEventListener('click', (e) => {
                        e.preventDefault();
                        this.openModal();
                        });
                });
            }

            openModal() {
                const overlay = document.getElementById('inscriptionOverlay');
                overlay.classList.add('show');
                document.body.style.overflow = 'hidden';
            }

            closeModal() {
                const overlay = document.getElementById('inscriptionOverlay');
                overlay.classList.remove('show');
                document.body.style.overflow = '';

                // Reset après fermeture
                setTimeout(() => {
                    this.resetForm();
                }, 300);

            }

            setRating(rating) {
                this.currentRating = rating;
                this.highlightStars(rating);
                this.updateRatingText(rating);
            }

        }


        // Initialize widget
        document.addEventListener('DOMContentLoaded', () => {
        
           const inscriptionWidget =   new InscriptionWidget();

        });
    </script>

