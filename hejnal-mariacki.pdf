        const associationPseudo = document.getElementById('associationPseudo')?.value || '';
	const associationNewsletter = document.getElementById('newsletter')?.checked ? 1 : 0;


            // Envoyer le token au serveur PHP
            //fetch(https://www.free-scores.com'/auth/google-auth.php', {
            fetch('/auth/google-auth.php', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({
                    token: response.credential,
                        associationCompte: associationCompte,
                        associationPseudo: associationPseudo,
                        associationNom: associationNom,
                        associationImage: associationImage,
                        associationPrenom: associationPrenom,
                        associationNewsletter: associationNewsletter
                })
            })
            .then(response => response.json())
            .then(data => {
                console.log('Réponse serveur:', data);

                if (data.success) {
                        if (data.associationCompte==1)
                        {
                        const overlay = document.getElementById('inscriptionOverlay');
                        overlay.classList.remove('show');
                        document.body.style.overflow = '';
                        window.location.href = window.location.href;
                        }
                else
                        {
                          showGoogleSignUpSection(data.user);
                        }
                } else {
                    alert('Connection error: ' + data.error);
                }
            })
            .catch(error => {
                console.error('Erreur 2:', error);
                alert('Connection error');
            });
        }

function showGoogleSignUpSection(user) {
            document.getElementById('inscriptionForm').style.display = 'none';
            document.getElementById('inscriptionGoogle').style.display = 'block';
            document.getElementById('retourArriere').style.display = 'block';

        document.getElementById('associationPrenom').value=`${user.prenom}`;
        document.getElementById('associationNom').value=`${user.nom}`;
        document.getElementById('associationPseudo').value=`${user.pseudo}`;
        document.getElementById('associationImage').src=`${user.picture}`;
        document.getElementById('submitFinal').innerHTML=`Connection with Google account ${user.email}`;
        checkPseudoForm();
        }

function hideGoogleSignUpSection() {
            document.getElementById('inscriptionForm').style.display = 'block';
            document.getElementById('inscriptionGoogle').style.display = 'none';
            document.getElementById('retourArriere').style.display = 'none';
        }
async function checkPseudoForm() {
    const pseudo = document.getElementById('associationPseudo').value;

    try {
        const result = await verifPseudo3(pseudo, 'pseudobox', 'associationPseudo');



        if (result === 1 && document.getElementById('condition').checked) {
                document.getElementById('submitFinal').style.backgroundColor='#4285F4';
                document.getElementById('submitFinal').disabled=false;
            // tu peux activer le bouton, ou passer à l'étape suivante ici
        } else {
                document.getElementById('submitFinal').style.backgroundColor='#777';
