<script>
   // SCRIPT POUR DISPARITION BARRE FIXE DU HAUT
   document.addEventListener('DOMContentLoaded', function() {
    let lastScrollTop = 0;
    const navbar = document.getElementById('navbar');
    const subMenu = document.querySelector('.header_sub_menu');
    const scrollThreshold = 5;

    if (!navbar) {
        console.error('Element #navbar non trouvé');
        return;
    }

    function handleScroll() {
        const currentScrollTop = window.pageYOffset || document.documentElement.scrollTop;
        
        if (Math.abs(currentScrollTop - lastScrollTop) < scrollThreshold) {
            return;
        }
        
        const isMobile = window.innerWidth <= 1300;
        
        if (currentScrollTop <= 0) {
            // En haut : header principal visible, sous-menu en position normale
            navbar.classList.remove('header-hidden');
            if (subMenu) {
                subMenu.style.top = isMobile ? '100px' : '60px';
            }
        }
        else if (currentScrollTop > lastScrollTop) {
            // Scroll down : CACHER le header principal, sous-menu remonte en haut
            navbar.classList.add('header-hidden');
            if (subMenu) {
                subMenu.style.top = '0px';
            }
        }
        else {
            // Scroll up : MONTRER le header principal, sous-menu redescend
            navbar.classList.remove('header-hidden');
            if (subMenu) {
                subMenu.style.top = isMobile ? '100px' : '60px';
            }
        }
        
        lastScrollTop = currentScrollTop;
    }

    let ticking = false;
    
    function requestTick() {
        if (!ticking) {
            requestAnimationFrame(() => {
                handleScroll();
                ticking = false;
            });
            ticking = true;
        }
    }

    window.addEventListener('scroll', requestTick, { passive: true });
    
    window.addEventListener('resize', () => {
        const isMobile = window.innerWidth <= 1300;
        if (subMenu && !navbar.classList.contains('header-hidden')) {
            subMenu.style.top = isMobile ? '100px' : '60px';
        } else if (subMenu && navbar.classList.contains('header-hidden')) {
            subMenu.style.top = '0px';
        }
    });
});
    </script>
